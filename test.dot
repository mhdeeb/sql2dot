    subgraph relationships {
        node [shape = diamond; fillcolor = "#7a7af3"; style = "rounded,filled"; color = black;];
    }
    
    subgraph entities {
        node [shape = box; fillcolor = "#43ce43"; style = "filled"; color = black;];
        
